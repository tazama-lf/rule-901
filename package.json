{
  "name": "@frmscoe/rule-901",
  "version": "2.0.0",
  "description": "Rule 901 is to calculate the number of transactions the debtor has made over a period",
  "main": "index.ts",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "@frmscoe:registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "npx rimraf lib node_modules coverage package-lock.json",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:eslint-unused": "eslint --report-unused-disable-directives \"**/*.ts\"",
    "lint:eslint-no-inline": "eslint --no-inline-config \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "dry": "npm publish --dry-run",
    "prepare": "husky",
    "test": "jest --config=jest.config.ts --passWithNoTests --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frmscoe/rule-901.git"
  },
  "keywords": [
    "rule-091"
  ],
  "author": "johanfol",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/frmscoe/rule-901/issues"
  },
  "homepage": "https://github.com/frmscoe/rule-901#readme",
  "dependencies": {
    "@frmscoe/frms-coe-lib": "4.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^52.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "husky": "9.0.11",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
